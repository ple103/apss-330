<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:mewmont"
             x:Class="mewmont.MainPage">

    <StackLayout Orientation="Vertical">
        <WebView x:Name="MediaViewer" Source="https://www.youtube.com/embed/wA-qqOP9p6Q" HeightRequest="{Binding MediaHeight}">
            <WebView.BindingContext>
                <local:RoomViewModel />
            </WebView.BindingContext>
        </WebView>
        <Label x:Name="debugInfo"></Label>
    </StackLayout>

</ContentPage>